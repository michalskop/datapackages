            }, {
                "description": "Party's name",
                "name": "name",
                "type": "string"
            }, {
                "description": "Party's abbreviation",
                "name": "abbreviation",
                "type": "string"
            }, {
                "description": "indicator whether the party answered to the VAA questions",
